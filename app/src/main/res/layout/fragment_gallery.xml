<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.autonsi.databoard.fragment.Homeinfosensor.HomeFragment">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/activity_vertical_margin">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:id="@+id/Mapbuild"
                style="@style/SelectableItemBackground"

                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_marginRight="8dp"
                android:layout_weight="1"
                android:background="@drawable/action_lick"
                android:foreground="?android:attr/selectableItemBackground"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/ima"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/ic_map" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ima"
                    android:layout_centerHorizontal="true"
                    android:text="Status Layout"
                    android:textColor="#fff" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/checksensor"
                style="@style/SelectableItemBackground"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_weight="1"
                android:background="@drawable/action_lick"
                android:foreground="?android:attr/selectableItemBackground"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/ima2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/ic_qccheck" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ima2"
                    android:layout_centerHorizontal="true"
                    android:text="Sensor List"
                    android:textColor="#fff" />
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:id="@+id/History"
                style="@style/SelectableItemBackground"

                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_marginRight="8dp"
                android:layout_weight="1"
                android:background="@drawable/action_lick"
                android:foreground="?android:attr/selectableItemBackground"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/eima"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/ic_report" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/eima"
                    android:layout_centerHorizontal="true"
                    android:text="Issues report"
                    android:textColor="#fff" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/issueslist"
                style="@style/SelectableItemBackground"

                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_weight="1"
                android:background="@drawable/action_lick"
                android:foreground="?android:attr/selectableItemBackground"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/eima2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/ic_alarm" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/eima2"
                    android:layout_centerHorizontal="true"
                    android:text="Issues List"
                    android:textColor="#fff" />
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>
